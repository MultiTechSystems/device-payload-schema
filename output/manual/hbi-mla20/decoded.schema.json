    },
    "device_status": {
      "type": "number"
    },
    "radar_switch": {
      "type": "number"
    },
    "daylight_switch": {
      "type": "number"
    },
    "emergency_switch": {
      "type": "number"
    },
    "intensity_switch": {
      "type": "number"
    },
    "l2": {
      "type": "number"
    },
    "l1": {
      "type": "number"
    },
    "led_intensity": {
      "type": "number"
    },
    "battery_voltage": {

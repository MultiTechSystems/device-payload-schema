{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://lorawan-schema.org/decoded/mclimate_flood_sensor",
  "title": "mclimate_flood_sensor Decoded Output",
  "description": "JSON Schema for validating decoded output from mclimate_flood_sensor sensor",
  "type": "object",
  "properties": {
    "reason": {
      "enum": {
        "0": "keepalive",
        "1": "testButtonPressed",
        "2": "floodDetected",
        "3": "fraudDetected"
      }
    },
    "boxTamper": {
      "type": "number"
    },
    "flood": {
      "type": "number"
    },
    "battery": {
      "type": "number",
      "description": "Unit: V"
    },
    "temperature": {
      "type": "number",
      "description": "Unit: \u00b0C"
    }
  },
  "additionalProperties": true
}
